<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.sys.entity.cfg.CfgTabledata" table="CFG_TABLEDATA">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="name" column="name" type="string" />
		<property name="tableName" column="table_name" type="string" />
		<property name="resourceName" column="resource_name" type="string" />
		<property name="tableType" column="table_type" type="integer" />
		<property name="parentTableId" column="parent_table_id" type="string" />
		<property name="parentTableName" column="parent_table_name" type="string" />
		<property name="isHavaDatalink" column="is_hava_datalink" type="integer" />
		<property name="subRefParentColumnId" column="sub_ref_parent_column_id" type="string" />
		<property name="subRefParentColumnName" column="sub_ref_parent_column_name" type="string" />
		<property name="comments" column="comments" type="string" />
		<property name="version" column="version" type="integer" />
		<property name="isDeploymentTest" column="is_deployment_test" type="integer" />
		<property name="isDeploymentRun" column="is_deployment_run" type="integer" />
	</class>
</hibernate-mapping>
