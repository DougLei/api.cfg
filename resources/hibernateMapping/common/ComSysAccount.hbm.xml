<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.sys.entity.common.ComSysAccount" table="COM_SYS_ACCOUNT">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="loginName" column="login_name" type="string" />
		<property name="loginPwd" column="login_pwd" type="string" update="false" />
		<property name="loginPwdKey" column="login_pwd_key" type="string" />
		<property name="tel" column="tel" type="string" />
		<property name="emails" column="emails" type="string" />
		<property name="accountType" column="account_type" type="integer" update="false" />
		<property name="accountStatus" column="account_status" type="integer" />
		<property name="accountStatusDes" column="account_status_des" type="string" />
		<property name="validDate" column="valid_date" type="timestamp" />
		<property name="isUnDelete" column="is_un_delete" type="integer" />
	</class>
</hibernate-mapping>
