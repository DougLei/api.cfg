<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.sys.entity.common.ComReqLog" table="COM_REQ_LOG">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="reqAccountId" column="req_account_id" type="string" />
		<property name="reqMethod" column="req_method" type="string" />
		<property name="reqIp" column="req_ip" type="string" />
		<property name="reqMac" column="req_mac" type="string" />
		<property name="reqUrl" column="req_url" type="string" />
		<property name="reqUrlParams" column="req_url_params" type="string" />
		<property name="reqBody" column="req_body" type="string" />
		<property name="respBody" column="resp_body" type="string" />
		<property name="reqDate" column="req_date" type="timestamp" />
		<property name="respDate" column="resp_date" type="timestamp" />
		<property name="reqToken" column="req_token" type="string" />
		<property name="projectId" column="project_id" type="string" />
	</class>
</hibernate-mapping>
