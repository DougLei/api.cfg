<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="ComUser" table="COM_USER">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="accountId" column="account_id" type="string" />
		<property name="nikeName" column="nike_name" type="string" />
		<property name="realName" column="real_name" type="string" />
		<property name="age" column="age" type="string" />
		<property name="sex" column="sex" type="string" />
		<property name="officePhone" column="office_phone" type="string" />
		<property name="tel" column="tel" type="string" />
		<property name="userEmail" column="user_email" type="string" />
		<property name="wrokAddr" column="wrok_addr" type="string" />
		<property name="liveAddr" column="live_addr" type="string" />
		<property name="idCardNo" column="id_card_no" type="string" />
		<property name="employedDate" column="employed_date" type="timestamp" />
		<property name="userStatus" column="user_status" type="string" />
		<property name="monthSalar" column="month_salar" type="string" />
		<property name="workNo" column="work_no" type="string" />
		<property name="secretLevel" column="secret_level" type="string" />
		<property name="descs" column="descs" type="string" />
	</class>
</hibernate-mapping>
