<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.sys.entity.common.ComPermission" table="COM_PERMISSION">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="refResourceId" column="ref_resource_id" type="string" />
		<property name="permissionType" column="permission_type" type="integer" />
		<property name="isVisibility" column="is_visibility" type="integer" />
		<property name="isOper" column="is_oper" type="integer" />
		<property name="secretLevels" column="secret_levels" type="string" />
		<property name="projectId" column="project_id" type="string" />
	</class>
</hibernate-mapping>
