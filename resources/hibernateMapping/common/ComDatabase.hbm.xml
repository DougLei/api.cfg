<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.sys.entity.common.ComDatabase" table="COM_DATABASE">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="dbDisplayName" column="db_display_name" type="string" />
		<property name="dbType" column="db_type" type="string" />
		<property name="dbInstanceName" column="db_instance_name" type="string" />
		<property name="loginUserName" column="login_user_name" type="string" />
		<property name="loginPassword" column="login_password" type="string" />
		<property name="dbIp" column="db_ip" type="string" />
		<property name="dbPort" column="db_port" type="integer" />
		<property name="cfgMainFileContent" column="cfg_main_file_content" type="string" />
		<property name="cfgTmplogFileContent" column="cfg_tmplog_file_content" type="string" />
		<property name="isEnabled" column="is_enabled" type="integer" />
		<property name="isDbCapacityErr" column="is_db_capacity_err" type="integer" />
		<property name="dbCapacityDesc" column="db_capacity_desc" type="string" />
		<property name="isCreated" column="is_created" type="integer" />
		<property name="projectId" column="project_id" type="string" />
	</class>
</hibernate-mapping>
