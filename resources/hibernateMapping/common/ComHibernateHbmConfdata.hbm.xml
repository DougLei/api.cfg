<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.king.tooth.sys.entity.common.ComHibernateHbmConfdata" table="COM_HIBERNATE_HBM_CONFDATA">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="tableId" column="table_id" type="string" />
		<property name="isDatalinkTableMapping" column="is_datalink_table_mapping" type="integer" />
		<property name="fileContent" column="file_content" type="string" />
		<property name="version" column="version" type="integer" />
		<property name="projectId" column="project_id" type="string" />
	</class>
</hibernate-mapping>
