<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="ComSysAccountOnlineStatus" table="COM_SYS_ACCOUNT_ONLINE_STATUS">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="accountId" column="account_id" type="string" />
		<property name="accountName" column="account_name" type="string" />
		<property name="token" column="token" type="string" />
		<property name="loginIp" column="login_ip" type="string" />
		<property name="loginMac" column="login_mac" type="string" />
		<property name="loginDate" column="login_date" type="timestamp" />
		<property name="tryLoginTimes" column="try_login_times" type="string" />
		<property name="lastOperDate" column="last_oper_date" type="timestamp" />
	</class>
</hibernate-mapping>
