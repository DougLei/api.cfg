<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="ComProjectModuleBody" table="COM_PROJECT_MODULE_BODY">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		<property name="createUserId" column="create_user_id" type="string" />
		<property name="lastUpdatedUserId" column="last_updated_user_id" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="lastUpdateTime" column="last_update_time" type="timestamp" />
		
		<property name="moduleId" column="module_id" type="string" />
		<property name="moduleBody" column="module_body" type="string" />
	</class>
</hibernate-mapping>	