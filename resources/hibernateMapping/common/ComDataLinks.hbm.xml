<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class entity-name="ComDataLinks" table="COM_DATA_LINKS">
		<id name="id" column="id" type="string">
			<generator class="assigned" />
		</id>
		
		<property name="leftId" column="left_id" type="string" />
		<property name="rightId" column="right_id" type="string" />
		<property name="orderCode" column="order_code" type="integer" />
		<property name="leftResourceName" column="left_resource_name" type="string" />
		<property name="rightResourceName" column="right_resource_name" type="string" />
		<property name="projectId" column="project_id" type="string" />
	</class>
</hibernate-mapping>
